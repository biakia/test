<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	
<package name="group1" extends="default">


<action name="chart.*" method="{1}"  class="com.manage.ssi.action.ChartAction">
		
<result name="jsp" type="dispatcher">/jsp/${target}.jsp</result>
		
</action>

<action name="account.*" method="{1}"  class="com.manage.ssi.action.AccountAction">
		
<result name="jsp" type="dispatcher">/jsp/${target}.jsp</result>
		
</action>


<action name="accounter.*" method="{1}"  class="com.manage.ssi.action.AccounterAction">
		
<result name="jsp" type="dispatcher">/jsp/${target}.jsp</result>
		
</action>			

<action name="userlogin.*" method="{1}"  class="com.manage.ssi.action.UserCheckInfoAction">
		
<result name="jsp" type="dispatcher">/jsp/${target}.jsp</result>
		
</action>			

</package>  


</struts>